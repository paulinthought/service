{
    "name": "paul/places",
    "description": "Install HTTP_Request2 dependency for places api calls and phpunit",
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "pear-pear.php.net/http_request2": "2.2.0",
        "behat/behat": "2.4.*@stable",
        "slim/slim": "2.6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "authors": [
        {
            "name": "Paul Grattan",
            "email": "paulgrattan@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PlaceFinder\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin/"
    }
}
